    <div class="container-fluid py-3">
       <header class="text-center">
           <h1 class="h2 mb-3 font-weight-normal" style="text-decoration:underline;">Result</h1>
       </header>
    </div>
   <section class="container my-2 w-50 p-2">
    <form class="row g-3 p-3">
        <div class="col-md-3">
            <label for="roll" class="form-label">Roll : </label>  
        </div>
        <div class="col-md-9">
            <input type="number" class="form-control" id="roll" name="roll" value="<%- user.Roll %>" readonly> 
        </div>


       <div class="col-md-3">
                <label for="name" class="form-label">Name :</label>
        </div>

        <div class="col-md-9">
                <input type="text" class="form-control" id="name" name="name" value="<%- user.Name %>" readonly>
        </div>

        <div class="col-md-3">
          <label for="dob" class="form-label">Date Of Birth :</label>
        </div>

        <div class="col-md-9">
            <input type="text" class="form-control" id="dob" name="dob" value="<%- user.Dob %>" readonly>
        </div>

        <div class="col-md-3">
              <label for="score" class="form-label">Score :</label>
        </div>

        <div class="col-md-9">
                <input type="Number" class="form-control" id="score" name="score" value="<%- user.Score %>" readonly>
        </div>

    </form>
    </section>
    